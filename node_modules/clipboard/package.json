{
  "name": "clipboard",
  "version": "1.4.2",
  "description": "Modern copy to clipboard. No Flash. Just 2kb",
  "repository": {
    "type": "git",
    "url": "https://github.com/zenorocha/clipboard.js"
  },
  "license": "MIT",
  "main": "dist/clipboard.js",
  "browser": "src/clipboard.js",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "loose": "all"
        }
      ]
    ]
  },
  "keywords": [
    "clipboard",
    "copy",
    "cut"
  ],
  "dependencies": {
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "delegate-events": "^1.1.1",
    "tiny-emitter": "^1.0.0"
  },
  "devDependencies": {
    "karma": "^0.13.10",
    "karma-browserify": "^4.4.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sinon": "^1.0.4",
    "mocha": "^2.3.3",
    "phantomjs-polyfill": "0.0.1",
    "uglify": "^0.1.5",
    "watchify": "^3.4.0"
  },
  "scripts": {
    "build": "npm run build-debug && npm run build-min",
    "build-debug": "browserify src/clipboard.js -s Clipboard -o dist/clipboard.js",
    "build-min": "uglify -s dist/clipboard.js -o dist/clipboard.min.js",
    "build-watch": "watchify src/clipboard.js -s Clipboard -o dist/clipboard.js -v",
    "test": "npm run test-browser && npm run test-server",
    "test-browser": "karma start --single-run",
    "test-server": "mocha test/module-systems.js"
  },
  "gitHead": "54efeb68e6bed239a853f85ebc4786fd62de613b",
